{
  "name": "football-manager-backend",
  "version": "0.0.1",
  "description": "backend part of Football Simulator",
  "author": "Oleksii Shkulipa",
  "license": "MIT",
  "keywords": [
    "ts",
    "nestjs",
    "Soccer",
    "Football",
    "Soccer Simulator",
    "Football Simulator"
  ],
  "engines": {
    "node": "18.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shkulipa/football-manager-backend"
  },
  "bugs": {
    "url": "https://github.com/Shkulipa/football-manager-backend/issues"
  },
  "homepage": "https://github.com/Shkulipa/football-manager-backend#readme",
  "scripts": {
    "commit": "cz",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json --port 8081 -s",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docker:compose": "docker-compose up -d",
    "start": "nest start",
    "prod": "node dist/main",
    "start:prod": "NODE_ENV=production nest start --watch",
    "start:stag": "NODE_ENV=staging nest start --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "migrate:help": "ts-node migrations.ts help",
    "migrate:prod:create": "NODE_ENV=production ts-node migrations.ts new -n",
    "migrate:prod:status": "NODE_ENV=production ts-node migrations.ts status",
    "migrate:prod:up": "NODE_ENV=production ts-node migrations.ts up",
    "migrate:prod:down": "NODE_ENV=production ts-node migrations.ts down",
    "migrate:dev:status": "NODE_ENV=development ts-node migrations.ts status",
    "migrate:dev:create": "NODE_ENV=development ts-node migrations.ts new -n",
    "migrate:dev:up": "NODE_ENV=development ts-node migrations.ts up",
    "migrate:dev:down": "NODE_ENV=development ts-node migrations.ts down",
    "migrate:stag:create": "NODE_ENV=staging ts-node migrations.ts new -n",
    "migrate:stag:status": "NODE_ENV=staging ts-node migrations.ts status",
    "migrate:stag:up": "NODE_ENV=staging ts-node migrations.ts up",
    "migrate:stag:down": "NODE_ENV=staging ts-node migrations.ts down",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.266.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^10.2.2",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/websockets": "^10.2.2",
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commitizen": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "footballsimulationengine": "^4.0.0",
    "highlight.js": "^11.7.0",
    "ioredis": "^5.3.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.1.2",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "migrate-mongo": "^9.0.0",
    "mime-types": "^2.1.35",
    "mongo-migrate-ts": "^1.1.1",
    "mongoose": "^6.9.0",
    "nestjs-pino": "^3.3.0",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.2",
    "stripe": "^13.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/markdown-it": "^12.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-facebook": "^3.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "stripe-event-types": "^2.4.0",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "~4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
